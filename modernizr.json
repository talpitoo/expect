{
  "classPrefix": "modernizr-",
  "options": [
    "addTest",
    "atRule",
    "domPrefixes",
    "hasEvent",
    "html5shiv",
    "html5printshiv",
    "load",
    "mq",
    "prefixed",
    "prefixes",
    "prefixedCSS",
    "setClasses",
    "testAllProps",
    "testProp",
    "testStyles"
  ],
  "feature-detects": [
    "css/supports",
    "css/transforms3d",
    "img/sizes",
    "img/srcset",
    "serviceworker",
    "touchevents"
  ]
}
